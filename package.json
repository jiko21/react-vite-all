{
  "name": "vitest-sample",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint './src/**/*.{ts,tsx}'",
    "lint:prettier": "prettier --check './src/**/*.{ts,tsx,css}'",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@emotion/css": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@storybook/addon-actions": "^6.5.0-alpha.34",
    "@storybook/addon-essentials": "^6.5.0-alpha.34",
    "@storybook/addon-links": "^6.5.0-alpha.34",
    "@storybook/react": "^6.5.0-alpha.34",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vitejs/plugin-react": "^1.0.7",
    "babel-loader": "^8.2.3",
    "eslint": "^8.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-storybook": "^0.5.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "storybook-builder-vite": "^0.1.14",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  }
}
